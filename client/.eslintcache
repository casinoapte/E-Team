ages/login/Login'\nimport Stats from './pages/stats/Stats'\nimport Shop from './pages/shop/Shop'\nimport Settings from './pages/settings/Settings'\n\n/* Game Pages */\n\nimport CSGO from './pages/csgo/CSGO'\nimport Dota from './pages/dota/Dota'\nimport LOL from './pages/lol/LOL'\nimport Fortnite from './pages/fortnite/Fortnite'\nimport Starcraft from './pages/starcraft/Starcraft'\n\n\n\n\n\n\n\nfunction App() {\n  return (\n\n    <Router>\n      <>\n\n        {/* SideBars */}\n\n        <Navbar />\n        <Sidebar />\n\n        <Switch>\n          <Route exact path=\"/\" component={Home}></Route>\n          <Route exact path=\"/test\" component={Test}></Route>\n          <Route exact path=\"/explore\" component={Explore}></Route>\n          <Route exact path=\"/profile\" component={Profile}></Route>\n          <Route exact path=\"/stats\" component={Stats}></Route>\n          <Route exact path=\"/shop\" component={Shop}></Route>\n          <Route exact path=\"/settings\" component={Settings}></Route>\n\n      \n\n          <Route exact path=\"/CSGO\" component={CSGO}></Route>\n          <Route exact path=\"/League-Of-Legends\" component={LOL}></Route>\n          <Route exact path=\"/Dota\" component={Dota}></Route>\n          <Route exact path=\"/Fortnite\" component={Fortnite}></Route>\n          <Route exact path=\"/Starcraft\" component={Starcraft}></Route>\n\n        </Switch>\n\n      </>\n\n    </Router>\n  );\n}\n\nexport default App;\n","/Users/casinoa/Desktop/Coding/ClassWork/Projects/E-Team-Practice/client/src/utils/API.js",["105"],"import axios from 'axios'\n\nexport default {\n\n    ///////////////////////\n    // Posts //\n    ///////////////////////\n\n\n    // Adding Post to DB //\n\n    addPost: function (postObject) {\n        return axios.post(\"/api/post/\", postObject)\n    },\n\n    // Getting Posts From DB //\n\n    getPost: function () {\n        return axios.get(\"/api/post/\")\n    },\n\n\n\n    ///////////////////////\n    // Drafts //\n    ///////////////////////\n\n    // Adding Draft to DB // \n\n    addDraft: function (draftObect) {\n        return axios.post(\"/api/draft/\", draftObect)\n    }\n\n\n\n}\n","/Users/casinoa/Desktop/Coding/ClassWork/Projects/E-Team-Practice/client/src/components/sideBar/Sidebar.js",["106"],"import React from 'react';\nimport styled from 'styled-components';\nimport { BrowserRouter as Route, Link, withRouter } from \"react-router-dom\";\n\nconst StyledSideNav = styled.div`\n    // margin-left: 1350px; //\n    position: fixed;     /* Fixed Sidebar (stay in place on scroll and position relative to viewport) */\n    height: 100%;\n    width: 75px;     /* Set the width of the sidebar */\n    z-index: 1;      /* Stay on top of everything */\n    top: 3.1em;      /* Makes it go all the way up (uncomment when using with a navbar) */\n    background-color: #222; /* Black */\n    overflow-x: hidden;     /* Disable horizontal scroll */\n    padding-top: 20px;\n`;\n\nclass SideNav extends React.Component {\n    constructor(props) {\n        super(props);\n        this.state = {\n            activePath: props.location.pathname,\n            items: [\n                {\n                    path: '/', /* path is used as id to check which NavItem is active basically */\n                    name: 'Home',\n                    css: 'fa fa-fw fa-home',\n                    key: 1 /* Key is required, else console throws error. Does this please you Mr. Browser?! */\n                },\n                {\n                    path: '/explore',\n                    name: 'explore',\n                    css: 'fa fa-search',\n                    key: 2\n                },\n                {\n                    path: '/profile',\n                    name: 'profile',\n                    css: 'fa fa-user-circle',\n                    key: 3\n                },\n                {\n                    path: '/stats',\n                    name: 'stats',\n                    css: 'fa fa-chart-bar',\n                    key: 4\n                },\n                {\n                    path: '/shop',\n                    name: 'shop',\n                    css: 'fa fa-shopping-bag',\n                    key: 5\n                },\n                {\n                    path: '/settings',\n                    name: 'settings',\n                    css: 'fa fa-cog',\n                    key: 6\n                },\n            ]\n        }\n    }\n\n    onItemClick = (path) => {\n        this.setState({ activePath: path });\n    }\n\n    render() {\n        const { items, activePath } = this.state;\n        return (\n            <StyledSideNav>\n                {\n                    items.map((item) => {\n                        return (\n                            <NavItem\n                                path={item.path}\n                                name={item.name}\n                                css={item.css}\n                                onItemClick={this.onItemClick}\n                                active={item.path === activePath}\n                                key={item.key}\n                            />\n                        );\n                    })\n                }\n            </StyledSideNav>\n        );\n    }\n}\n\nconst RouterSideNav = withRouter(SideNav);\n\nconst StyledNavItem = styled.div`\n    height: 70px;\n    width: 75px; /* width must be same size as NavBar to center */\n    text-align: center; /* Aligns <a> inside of NavIcon div */\n    margin-bottom: 0;   /* Puts space between NavItems */\n    a {\n        font-size: 2.7em;\n        color: ${(props) => props.active ? \"white\" : \"#9FFFCB\"};\n        :hover {\n            opacity: 0.7;\n            text-decoration: none; /* Gets rid of underlining of icons */\n        }  \n    }\n`;\n\nclass NavItem extends React.Component {\n    handleClick = () => {\n        const { path, onItemClick } = this.props;\n        onItemClick(path);\n    }\n\n    render() {\n        const { active } = this.props;\n        return (\n            <StyledNavItem active={active}>\n                <Link to={this.props.path} className={this.props.css} onClick={this.handleClick}>\n                    <NavIcon></NavIcon>\n                </Link>\n            </StyledNavItem>\n        );\n    }\n}\n\nconst NavIcon = styled.div`\n`;\n\nexport default class Sidebar extends React.Component {\n    render() {\n        return (\n            <RouterSideNav></RouterSideNav>\n        );\n    }\n}","/Users/casinoa/Desktop/Coding/ClassWork/Projects/E-Team-Practice/client/src/components/navBar/NavBar.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Projects/E-Team-Practice/client/src/pages/login/Login.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Projects/E-Team-Practice/client/src/pages/explore/Explore.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Projects/E-Team-Practice/client/src/pages/stats/Stats.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Projects/E-Team-Practice/client/src/pages/shop/Shop.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Projects/E-Team-Practice/client/src/pages/settings/Settings.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Projects/E-Team-Practice/client/src/pages/home/Test.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Projects/E-Team-Practice/client/src/pages/home/Home.js",["107","108"],"import React from 'react'\nimport GameLogos from '../../components/gameLogo/GameLogos'\nimport TopicButtons from '../../components/topic-btns/TopicBtns'\nimport CreatePost from '../../components/add-post/CreatePost'\n\n\nexport default function Home() {\n    return (\n        <div>\n            <CreatePost/>\n            {/* <TopicButtons/> */}\n            {/* <GameLogos/> */}\n\n\n            \n        </div>\n    )\n}\n","/Users/casinoa/Desktop/Coding/ClassWork/Projects/E-Team-Practice/client/src/components/game-logos/GameLogos.js",["109","110","111","112","113","114"],"import React from 'react'\nimport './styles.css'\nimport { useHistory } from 